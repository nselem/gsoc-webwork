# MENU FUNCIONES


@OtraVariable= (Formula("($a8*t^$a8+$a8)^{1/$a9}"),
                Formula("$a8*tan(e^{-t})+t^{-2}"),
                Formula("$a8*sqrt(t)+(sqrt(t))^{-1}"),
                Formula("($a8*e^t)/t"),
                Formula("(t^2)*csc($a8*t^3)"));

$f8=$OtraVariable[$I8]; $Df8 = $f8->D;
